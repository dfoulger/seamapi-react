{
  "name": "@seamapi/seam-components",
  "version": "1.1.0",
  "description": "Zero backend drop-in beautiful pre-built components with everything you need to get started with Seam",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/seamapi/seam-components.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup"
  },
  "author": "Azat Mardan",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/seamapi/seam-components/issues"
  },
  "homepage": "https://github.com/seamapi/seam-components#readme",
  "devDependencies": {
    "@types/react": "^18.0.30",
    "prettier": "^2.8.7",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
  },
  "dependencies": {
    "react": "^18.2.0",
    "seamapi": "^5.11.0"
  },
  "tsup": {
    "entry": ["src/**/*.ts", "src/**/*.tsx"],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs",
      "iife"
    ],
    "external": [
      "react"
    ],
    "minify": false,
    "bundle": true,
    "skipNodeModulesBundle": true,
    "watch": true

  }
}
