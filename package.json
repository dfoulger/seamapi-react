{
  "name": "@seamapi/react",
  "version": "0.0.0",
  "description": "Seam React component library.",
  "type": "module",
  "main": "index.js",
  "typings": "index.d.ts",
  "exports": {
    "import": "./index.js"
  },
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/seamapi/react",
  "bugs": "https://github.com/seamapi/react/issues",
  "repository": "seamapi/react",
  "license": "MIT",
  "author": {
    "name": "Seam Labs, Inc.",
    "email": "devops@getseam.com"
  },
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "lib",
    "src",
    "!test",
    "!**/*.test.ts"
  ],
  "scripts": {
    "build": "npm run build:ts",
    "postbuild": "node ./index.js",
    "build:ts": "tsc --project tsconfig.build.json",
    "prebuild:ts": "del 'index.*' lib",
    "postbuild:ts": "tsc-alias --project tsconfig.build.json",
    "typecheck": "tsc",
    "dependencies": "dpdm --exit-code circular:1 src/index.ts",
    "test": "true",
    "test:update": "true",
    "test:watch": "true",
    "test:debug": "true",
    "lint": "eslint --ignore-path .gitignore --ext .js,.cjs,.mjs,.ts,.tsx .",
    "prelint": "prettier --check --ignore-path .gitignore .",
    "postversion": "git push --follow-tags",
    "example:basic": "vite --config examples/basic/vite.config.ts",
    "format": "eslint --ignore-path .gitignore --fix --ext .js,.cjs,.mjs,.ts,.tsx .",
    "preformat": "prettier --write --ignore-path .gitignore .",
    "report": "c8 report"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">= 8.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "seamapi": ">=5.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "c8": "^7.11.0",
    "del-cli": "^5.0.0",
    "dpdm": "^3.13.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "seamapi": "^5.11.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.8"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.28.0"
  }
}
